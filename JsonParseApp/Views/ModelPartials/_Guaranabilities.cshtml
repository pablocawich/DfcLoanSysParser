@model IList<JsonParseApp.Models.Liability>

<div class="col-md-6">
    <h6>Liabilities</h6>
    <table class="table table-dark table-borderless">
        <thead>
        <tr>
            <th scope="col">Code</th>
            <th scope="col">Amount</th>
            <th scope="col">Description</th>
        </tr>
        </thead>
        <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@Html.TextBoxFor(l=>Model[i].IndividualFinancialsCode, new { @readonly = "readonly", @class = "form-control"})</td>
                <td>@Html.TextBoxFor(l=>Model[i].IndividualFinancialsAmount, new {@readonly = "readonly", @class = "form-control" })</td>

                <td>@Html.TextBoxFor(l=> Model[i].IndividualFinancialsDescription, new {@readonly = "readonly", @class = "form-control" })</td>
             @Html.HiddenFor(m=> Model[i].LoanApplicantProfileId)
             @Html.HiddenFor(m=> Model[i].GuarantorFinancialId)
            </tr>

        }
        </tbody>
    </table>

</div>