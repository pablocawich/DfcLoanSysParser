@model JsonParseApp.Models.MinorProfile

<div class="alert alert-danger" role="alert">
    <div class="form-group">
        <div class="form-check">
            <input class="form-check-input" type="checkbox" id="minorProfileIdCheck" value="">
            <p>Exclude from Import</p>
        </div>
    </div>
    <div class="form-group" id="minorIdBox" hidden>
        <p><i>Ensure a valid user ID is provided.</i></p>
        @Html.LabelFor(m => m.MinorId, new { @class = "control-label" })
        <div class="form-inline">
            @Html.TextBoxFor(m => m.MinorId, new { @class = "form-control app-id" })
            <button type="button" class="btn btn-sm btn-outline-primary validateBtn"><i class="fa fa-exclamation-circle"></i> Validate</button>
            <span id="" style="left: 305px;position: absolute;"></span>
        </div>
        
        <span class="emssg hidden">you have exceeded the required amount</span>
    </div>
</div>

